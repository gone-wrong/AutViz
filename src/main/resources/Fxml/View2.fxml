<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>

<AnchorPane fx:id="view2_parent" prefHeight="750.0" prefWidth="850.0" stylesheets="@../Styles/View2.css"
            xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sk.ukf.autviz.Controllers.View2Controller">
   <HBox prefHeight="50.0" prefWidth="850.0" styleClass="header_hbox" AnchorPane.leftAnchor="0.0"
         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <Button fx:id="add_state_button" mnemonicParsing="false" text="AddState" />
      <Button fx:id="delete_state_button" mnemonicParsing="false" text="DeleteState" />
      <Button fx:id="add_edge_button" mnemonicParsing="false" text="AddEdge" />
   </HBox>
   <AnchorPane fx:id="table_region" prefHeight="700.0" prefWidth="850.0" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
      <TableView fx:id="transition_table" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0"/>
   </AnchorPane>
</AnchorPane>
